function expForLevel(level, [difficulty = 1]) {
  assert(level > 0)
  return ((difficulty * (level * level * level)) ~/ 5 + 1) * 100
}

for (const i in range(1, 10)) {
  print(expForLevel(i))
}

print('-----')

function expForLevel2(level) {
  assert(level > 0)
  return (level * level + level) ~/ 2 * 100 - (level * 100)
}

for (const i in range(1, 10)) {
  print(expForLevel2(i))
}
