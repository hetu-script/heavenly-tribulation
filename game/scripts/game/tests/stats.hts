const random = Random()

for (const i in range(10)) {
  const maxStats = random.nextInt(10) + 10
  const minValue = 10

  const strength = random.nextInt(maxStats)
  const intelligence = random.nextInt(maxStats - strength)
  const perception = random.nextInt(maxStats - strength - intelligence)
  const superpower = random.nextInt(maxStats - strength - intelligence - perception)
  const leadership = random.nextInt(maxStats - strength - intelligence - perception - superpower)
  const management = maxStats - strength - intelligence - perception - superpower - leadership

  const stats = {
    strength: strength + minValue,
    intelligence: intelligence + minValue,
    perception: perception + minValue,
    superpower: superpower + minValue,
    leadership: leadership + minValue,
    management: management + minValue,
  }
  print(maxStats)
  print(stats)
}
