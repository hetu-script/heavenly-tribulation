  // 最终的卡牌上可能还会有一些其他的属性
  // 被诅咒的卡牌，必然出现在战斗卡组中，并且自身的卡组数量上限减一
  // isCursed: false,
  // 被腐化的卡牌，无法再进行精炼
  // isCorrupted: false,

  blank_default: {
    id: "blank_default",
    description: "blank_default",
    image: "battlecard/illustration/blank_default.png",
    // 绝世卡牌的词条是固定的且无法修改
    isUnique: true,
    // 不会出现在卡包中
    isHidden: true,
  },

  // ------------攻击卡牌------------
  attack_punch: {
    // 不同的词条可能有相同的affixId，非null的affixId只会在一张卡牌上出现一次。
    id: "attack_punch",
    // 和 support_affix 不同，主词条的 uniqueId [是]数组，category [不是] 数组
    // attack 攻击类的卡牌没有uniqueId
    // buff 加持类才有
    // uniqueId: null,
    // 效果类型，分为攻击（attack），加持（buff）两种
    category: "attack",
    // 流派，御剑，悟道，炼魂等，如果为null表示无流派
    // isEphemeral 代表此卡牌是否在卡组中只能使用一次，卡组再次轮到时会跳过
    // isEphemeral: false,
    // 流派仅表示获得方法，使用时并无限制
    // genre: null,
    // kind决定了卡牌名字类型，例如剑法，咒语等等
    // 虽然并非强制，但多数时候kind和战斗动画也是对应的
    kind: "punch",
    // 词条出现的几率，如果 roll 值大于几率，会重新选择词条
    // chance: 0.4,
    // 攻击类型，一共五种，徒手，武器，法术，咒语，混沌
    attackType: "unarmed",
    // 伤害类型，一共五种，物理，真气，元素，精神，纯粹
    damageType: "physical",
    // 使用卡牌需要符合境界要求
    // 境界影响卡牌的额外词条数量和种类以及卡牌等级上限
    // rank: 0,
    description: "affix_attack_unarmed",
    // 卡牌插画
    image: "battlecard/illustration/attack_punch.png",
    animation: {
      // 前摇动画
      startup: ["before_melee_startup", "melee_startup"],
      // 后摇动画（可选）
      recovery: ["melee_recovery", "after_melee_recovery"],
      // 过渡动画组（可选）
      actions: "attack_punch",
      // 叠加动画（爆炸效果等，可选）
      // overlays: [],
      // 动画播放时的声音效果
      sound: "punch-or-kick-sound-effect-1-239696.mp3",
    },
    // 调用的脚本函数的id
    script: "attack",
    // 是否是主词条，主词条会直接写在卡面上，额外词条会在光标浮窗中显示
    // 主词条不需要鉴定，额外词条签订前没有效果并且显示为问号
    // 只有主词条上会出现装备需求
    equipment: null,
    // 实际的数值会在游戏中实际创建时才添加到这里
    // 主词条等级始终和卡牌等级一致
    // level: 0,
    // value: [0]
    valueData: [
      {
        // 实际值=base+level*increment
        // 每个值可能有自己的maxLevel，大多数速度，闪避类的加成最高40级
        base: 7,
        increment: 0.7,
      },
    ],
  },
