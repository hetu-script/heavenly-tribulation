struct TileMapObject {
  construct ({
    terrainIndex,
    left,
    top,
    velocityFactor = 0.5,
    spriteSrc,
    srcWidth,
    srcHeight,
    entityId,
  }) {
    this.terrainIndex = terrainIndex
    this.left = left
    this.top = top
    this.velocityFactor = velocityFactor
    this.spriteSrc = spriteSrc
    this.srcWidth = srcWidth
    this.srcHeight = srcHeight
    this.entityId = entityId
  }
}
