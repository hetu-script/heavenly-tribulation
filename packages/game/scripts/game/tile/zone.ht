import '../random.ht'
import '../entity/common.ht'
import '../name/zone.ht'

struct Zone {
  construct ({index, name, category}) {
    assert(index != null)
    assert(category != null)
    this.index = index
    this.name = name
    this.category = category
    this.color = random.nextBrightColorHex()
    this.terrainIndexes = []
  }
}
