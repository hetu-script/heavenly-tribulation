final kLocationJailVisitPermit = 'locationJailVisitPermit'

const kUnknownItemIcon = 'icon/unknown_item.png'

struct CommonItem {
  construct ({
    id,
    name,
    description,
    icon,
  }) {
    assert(id != null)
    assert(name != null)
    this.id = id
    this.name = name
    this.description = description
    this.icon = icon ?? kUnknownItemIcon
  }
}
