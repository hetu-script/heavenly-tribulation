
// 组织中每个等级的人数上限
// 数字越大，等级越低，0是掌门
fun maxMemberOfRank(n: int) {
  return (n + 1) * (n + 1)
}

// 组织发展度，等于组织可以拥有的境界上限
// 至少是 1，表示可以拥有 筑基 1 人，和 炼气 4 人
fun maxMemberOfDevelopment(n: int) {
  var list = []
  for (final i in range(0, n + 2)) {
    list.add(maxMemberOfRank(i))
  }
  return list
}

for (var i in range(0, 10)) {
  print(i, maxMemberOfRank(i), maxMemberOfDevelopment(i))
}