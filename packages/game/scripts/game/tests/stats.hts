final random = Random()

for (var i in range(10)) {
  final maxStats = random.nextInt(10) + 10
  final minValue = 10

  final strength = random.nextInt(maxStats)
  final intelligence = random.nextInt(maxStats - strength)
  final perception = random.nextInt(maxStats - strength - intelligence)
  final superpower = random.nextInt(maxStats - strength - intelligence - perception)
  final leadership = random.nextInt(maxStats - strength - intelligence - perception - superpower)
  final management = maxStats - strength - intelligence - perception - superpower - leadership

  final stats = {
    strength: strength + minValue,
    intelligence: intelligence + minValue,
    perception: perception + minValue,
    superpower: superpower + minValue,
    leadership: leadership + minValue,
    management: management + minValue,
  }
  print(maxStats)
  print(stats)
}
